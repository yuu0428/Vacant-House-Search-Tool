{
  "name": "walktrace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "deploy:cf": "npm run build && wrangler pages deploy dist --project-name walktrace",
    "serve": "npm run build && serve dist --listen 5173 --single",
    "tunnel": "npm run build && node scripts/tunnel.mjs"
  },
  "dependencies": {
    "firebase": "^12.3.0",
    "idb": "^8.0.3",
    "jszip": "^3.10.1",
    "maplibre-gl": "^5.8.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/geojson": "^7946.0.16",
    "@types/jszip": "^3.4.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "localtunnel": "^2.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "serve": "^14.2.5",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.42.0"
  }
}
